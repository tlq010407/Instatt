<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.New" parent="Theme.Material3.DayNight.NoActionBar">
    </style>

    <style name="Theme.New" parent="Base.Theme.New" />
    <style name="Theme.New.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.New.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.New.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="notificationsSwitch" parent="Theme.AppCompat.Light">
    <item name="colorControlActivated">@color/Notification</item>
    <item name="colorSwitchThumbNormal">#FFFFFF</item>
    <item name="android:colorForeground">#99FFFFFF</item>
</style>
    <style name="CircularDeterminateProgressBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/circle</item>
    </style>

</resources>